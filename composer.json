{
    "name": "jacques/ofxparser",
    "description": "Simple OFX file parser",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "ofx",
        "open financial exchange",
        "finance",
        "parser"
    ],
    "authors": [
        {
            "name": "Guillaume Bailleul",
            "email": "contact@guillaume-bailleul.fr",
            "homepage": "http://www.guillaume-bailleul.fr"
        },
        {
            "name": "Jacques Marneweck",
            "email": "jacques@siberia.co.za",
            "homepage": "http://www.siberia.co.za/"
        },
        {
            "name": "James Titcumb",
            "email": "hello@jamestitcumb.com",
            "homepage": "http://www.jamestitcumb.com/"
        },
        {
            "name": "Oliver Lowe",
            "email": "mrtriangle@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-simplexml": "*",
        "composer/pcre": "^3.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.28",
        "phpunit/phpunit": "^9.5",
        "rector/rector": "^0.13.10",
        "squizlabs/php_codesniffer": "^3.7",
        "vimeo/psalm": "^4.26"
    },
    "autoload": {
        "psr-0": {
            "OfxParser": "lib/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true,
            "composer/package-versions-deprecated": true
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "replaces": [
        "asgrim/ofxparser"
    ]
}
